# Переменные для n8n
N8N_HOST="yourHost"
N8N_ENCRYPTION_KEY="generate-a-strong-random-string-for-this"
WEBHOOK_URL="https://yourHost"
GENERIC_TIMEZONE="Europe/Moscow"

# ID пользователя Telegram для административных сообщений (опционально)
TELEGRAM_ADMIN_CHAT_ID=""

# API ключи
TELEGRAM_TOKEN=""
TAVILY_API_KEY=""

# Ollama (LLM + embeddings)
OLLAMA_MODEL="llama3.2:3b"
# Для локального инстанса — http://ollama:11434, для облака — URL вашего провайдера
OLLAMA_BASE_URL="http://ollama:11434"
# Для Ollama Cloud/провайдера (если требуется)
OLLAMA_API_KEY=""
# Модель эмбеддингов (может отличаться от чат-модели)
OLLAMA_EMBEDDING_MODEL="nomic-embed-text"
# Лимит символов для prompt эмбеддингов
OLLAMA_EMBEDDING_MAX_CHARS="2048"
# Лимит символов для итогового промпта в чат
OLLAMA_PROMPT_MAX_CHARS="8000"

# Настройки Traefik
ACME_EMAIL="your-email@your-domain.com"