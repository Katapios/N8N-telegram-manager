# Переменные для n8n
N8N_HOST="yourHost"
N8N_ENCRYPTION_KEY="generate-a-strong-random-string-for-this"
WEBHOOK_URL="https://yourHost"
GENERIC_TIMEZONE="Europe/Moscow"

# ID пользователя Telegram для административных сообщений (опционально)
TELEGRAM_ADMIN_CHAT_ID=""

# API ключи
TELEGRAM_TOKEN=""
TAVILY_API_KEY=""

# Ollama (LLM + embeddings)
OLLAMA_MODEL="llama3.2:3b"
# Для локального инстанса — http://ollama:11434, для облака — URL вашего провайдера
OLLAMA_BASE_URL="http://ollama:11434"
# Для Ollama Cloud/провайдера (если требуется)
OLLAMA_API_KEY=""
# Модель эмбеддингов (может отличаться от чат-модели)
OLLAMA_EMBEDDING_MODEL="nomic-embed-text"
# Лимит символов для prompt эмбеддингов
OLLAMA_EMBEDDING_MAX_CHARS="2048"
# Лимит символов для итогового промпта в чат
OLLAMA_PROMPT_MAX_CHARS="8000"

# Настройки Traefik
ACME_EMAIL="your-email@your-domain.com"

# Self-hosted Supabase (step 1: database)
SUPABASE_DB_NAME="postgres"
SUPABASE_DB_USER="postgres"
SUPABASE_DB_PASSWORD="change-me-strong-password"

# Self-hosted Supabase (full stack)
SUPABASE_PUBLIC_URL="http://localhost:8000"
SUPABASE_SITE_URL="http://localhost:3000"
SUPABASE_ADDITIONAL_REDIRECT_URLS=""
SUPABASE_DISABLE_SIGNUP="false"
SUPABASE_MAILER_AUTOCONFIRM="true"
SUPABASE_JWT_EXP="3600"
SUPABASE_JWT_SECRET="replace-with-very-long-random-jwt-secret-at-least-32-chars"
SUPABASE_ANON_KEY="replace-with-actual-anon-jwt"
SUPABASE_SERVICE_ROLE_KEY="replace-with-actual-service-role-jwt"
SUPABASE_SECRET_KEY_BASE="replace-with-very-long-random-secret-key-base"

# Optional SMTP for auth emails
SUPABASE_SMTP_HOST=""
SUPABASE_SMTP_PORT="587"
SUPABASE_SMTP_USER=""
SUPABASE_SMTP_PASS=""
SUPABASE_SMTP_ADMIN_EMAIL=""
SUPABASE_SMTP_SENDER_NAME="LazyBones"

# Studio/logging (optional)
SUPABASE_STUDIO_DEFAULT_ORG="LazyBones"
SUPABASE_STUDIO_DEFAULT_PROJECT="LazyBones Local"
SUPABASE_LOGFLARE_API_KEY=""
SUPABASE_LOGFLARE_URL=""

# Edge Functions
SUPABASE_FUNCTIONS_VERIFY_JWT="false"

# Storage
SUPABASE_STORAGE_FILE_SIZE_LIMIT="52428800"
